{% extends 'layout/layout' %}

{% block title %}
	Download
{% endblock title %}

{% block content %}
	{% for iKey, oCurDownload in aDownloadList %}
		<a href="{{ oCurDownload.getLink }}" target="_blank">
			<div class="col-md-3 col-sm-6">
				<div class="servive-block-in">
					<h4>{{ oCurDownload.getHoster }}</h4>
					<div><i class="icon-lightbulb"></i></div>
				</div>
			</div>
		</a>
	{% else %}
		<p class="text-center">.... uploading ....</p>
	{% endfor %}
{% endblock content %}